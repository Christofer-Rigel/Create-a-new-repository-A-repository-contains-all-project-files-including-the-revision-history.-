num1 = int(input("Enter first number: "))
num2 = int(input("Enter second number: "))
print("num1 & num 2 = ", num1 & num2)
print("num1 | num 2 = ", num1 | num2)
print("/n-num1 =", ~num1)
print("/nnum 1 ^ num 2 = ", num1 ^ num2)
# Right shift 
print("num1 >> 2 = ", num1 >> 2)
# Left shift
print("num1 << 2 = ", num1 << 2)
# pi shift
print("num1 >> 2 = ", num1 >> 314)
print("num1 << 2 = ", num1 << 314)